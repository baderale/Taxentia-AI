# OpenAI Configuration (Used for embeddings and fallback/validation)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL_NAME=gpt-4o-mini

# Ollama Configuration (Primary LLM - Law-Specific Model)
OLLAMA_API_URL=http://localhost:11434
OLLAMA_MODEL=initium/law_model:Q4_K_S
OLLAMA_REQUEST_TIMEOUT=90000
USE_GPT4O_VALIDATION=true

# Qdrant Vector Database
QDRANT_URL=http://localhost:6333
QDRANT_COLLECTION_NAME=taxentia-authorities

# PostgreSQL Database
DATABASE_URL=postgresql://taxentia_user:taxentia_password@localhost:5432/taxentia
POSTGRES_DB=taxentia
POSTGRES_USER=taxentia_user
POSTGRES_PASSWORD=taxentia_password

# GovInfo API (for IRC sync)
GOVINFO_API_KEY=your_govinfo_api_key_here

# Application
NODE_ENV=development
PORT=5000

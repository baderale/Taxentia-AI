# OpenAI Configuration (Used for embeddings and primary LLM)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL_NAME=gpt-4o-mini

# Qdrant Vector Database
QDRANT_URL=http://localhost:6333
QDRANT_COLLECTION_NAME=taxentia-authorities

# PostgreSQL Database
DATABASE_URL=postgresql://taxentia_user:taxentia_password@localhost:5432/taxentia
POSTGRES_DB=taxentia
POSTGRES_USER=taxentia_user
POSTGRES_PASSWORD=taxentia_password

# GovInfo API (for IRC sync)
GOVINFO_API_KEY=your_govinfo_api_key_here

# Application
NODE_ENV=development
PORT=5000
